smoothingFactor <- c(2,10,100)
xTime = seq(-10,10,by=0.01)
xDays = seq(-183,183,by=1)
xDist = seq(-700,700,by=1)
iterList <- c(xTime, xDays, xDist)
kernel <- function(value) {
return (exp(-(abs(value)**2)))
}
par(mfrow=c(1,3))
iter <- 1
for(factor in smoothingFactor) {
y <- c()
x <- iterList[iter]
for (i in x) {
val <- kernel(i / factor)
y <- c(y, val)
}
plot(x, y, type='l', xlab=factor, ylim = c(0,1))
iter <- iter + 1
}
